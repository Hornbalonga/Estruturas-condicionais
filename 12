#include <stdio.h>

int val, k, c, v[8] = {200,100,50,20,10,5,2,1}, cont;
int main(){
  printf("digite um valor");
  scanf("%d", &val);
  for(cont = 0; cont <= 8; cont++){ 
      k = val/v[cont];
      val -= k*v[cont];
        if(k != 0){
          printf("%d notas de %d\n",k, v[cont]);
    }
  }
}
